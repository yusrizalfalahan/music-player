<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Music</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

</head>

<body>
  <div class="container">
    <h1><%= title %></h1>
    <a href="/create" class="btn btn-primary float-right" style="margin-bottom: 10px">Create</a>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">No</th>
          <th scope="col">ArtistName</th>
          <th scope="col">PackageName </th>
          <th scope="col">ImageURL</th>
          <th scope="col">ReleaseDate</th>
          <th scope="col">Audio</th>
          <th scope="col">Price</th>
          <!-- <th scope="col">Action</th> -->

        </tr>
      </thead>
      <tbody>
        <%  data.forEach(function (data, index)   { %>
        <tr>
          <th scope="row"><%= index + 1 %> </th>
          <th scope="row"><%= data.ArtistName %> </th>
          <th scope="row"><%= data.PackageName %> </th>
          <th scope="row"><%= data.ImageURL %> </th>
          <th scope="row"><%= data.ReleaseDate %> </th>
          <th scope="row"><%= data.SampleURL %> </th>
          <th scope="row"><%= data.Price %> </th>
          <th scope="row" a> <a href="<%= data.id %>/update" class="btn btn-success">Update</a> <a href="/<%= data.id %>/delete" class="btn btn-danger">Delete</a></th>

        </tr>
        <% }) %>



      </tbody>
    </table>


  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

</body>

</html>